{
  "name": "@raptrx/askai",
  "version": "1.4.0",
  "description": "Simplified AI prompt deep-linking for ChatGPT, Claude, Gemini, Grok, Perplexity and more",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "askai": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/react": "^18.2.0",
    "eslint": "^8.55.0",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^1.1.0"
  },
  "author": "Ali Arain",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliarain/askai.git"
  },
  "keywords": [
    "ai",
    "chatgpt",
    "claude",
    "gemini",
    "grok",
    "perplexity",
    "deepseek",
    "mistral",
    "prompt",
    "deep-link",
    "react",
    "cli"
  ],
  "sideEffects": false
}